<!-- Markup options. -->
add-xml-decl: no
add-xml-space: no
alt-text:
anchor-as-name: no
assume-xml-procins: no
bare: no
clean: yes
coerce-endtags: yes
css-prefix: c
decorate-inferred-ul: no
doctype: omit
drop-empty-elements: yes
drop-empty-paras: yes
drop-proprietary-attributes: no
enclose-block-text: no
enclose-text: no
escape-cdata: no
fix-backslash: yes
fix-bad-comments: yes
fix-uri: no
gdoc: no
hide-comments: no
hide-endtags: no
indent-cdata: no
input-xml: no
join-classes: no
join-styles: yes
literal-attributes: no
logical-emphasis: yes
lower-literals: yes
merge-divs: auto
merge-emphasis: yes
merge-spans: auto
ncr: yes
new-blocklevel-tags:
new-empty-tags:
new-inline-tags:
new-pre-tags:
numeric-entities: no
omit-optional-tags: no
output-html: no
output-xhtml: no
output-xml: no
preserve-entities: no
quote-ampersand: yes
quote-marks: no
quote-nbsp: yes
repeated-attributes: keep-last
replace-color: no
show-body-only: no
skip-nested: yes
strict-tags-attributes: no
uppercase-attributes: no
uppercase-tags: no
word-2000: no


<!-- Diagnostics options. -->
accessibility-check: 0 (Tidy Classic)
show-errors: 6
show-info: yes
show-warnings: yes


<!-- Pretty print options. -->
break-before-br: no
escape-scripts: yes
indent: no
indent-attributes: no
indent-spaces: 4
indent-with-tabs: yes
markup: yes
punctuation-wrap: no
sort-attributes: alpha
tab-size: 4
vertical-space: yes
wrap: 72
wrap-asp: no
wrap-attributes: no
wrap-jste: no
wrap-php: no
wrap-script-literals: no
wrap-sections: yes


<!-- Character encoding options. -->
ascii-chars: no
char-encoding: utf8
input-encoding: utf8
newline: Platform dependent
output-bom: auto
output-encoding: utf8


<!-- Miscellaneous options. -->
error-file:
force-output: no
gnu-emacs: no
keep-time: yes
quiet: no
tidy-mark: yes
write-back: no
